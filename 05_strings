const name = "GOURAV"

const repoCount = 55

// console.log(name + repoCount + " value");

console.log(`hello my name is ${name} and my repCount is ${repoCount}`);

const gameName = new String ("gourav labana")
console.log (gameName[0]);
console.log (gameName.__proto__);

console.log(gameName.toUpperCase());

const newString = gameName.substring(0, 4)
console.log(newString);

const anotherString = gameName.slice(0, 4)
console.log(anotherString);


const newStringOne = "   gourav   "
console.log(newStringOne.trim());

const url = "https://gourav%5labana"

console.log(url);
console.log(url.replace('%5', '-'));
console.log(url.includes('%5'));